{
  "name": "dxid",
  "version": "2.3.0",
  "description": "A better and safer way to display your primary keys in urls or to the users",
  "bin": "./index.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./index.d.ts"
    }
  },
  "run": {
    "dxid": "./index.js"
  },
  "scripts": {
    "build": "node src/esbuild.js",
    "prepublishOnly": "npm run build",
    "id": "./index.js",
    "dxid": "./index.js",
    "test": "ava",
    "lint": "npx biome lint --fix",
    "format": "npx biome format --fix",
    "check": "npx biome check --fix"
  },
  "files": [
    "src",
    "dist",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tttp/dxid.git"
  },
  "keywords": [
    "id",
    "luhn",
    "base32",
    "base64",
    "uuid"
  ],
  "author": "xavier dutoit",
  "type": "module",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tttp/dxid/issues"
  },
  "homepage": "https://dxid.tttp.eu",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "ava": "^5.3.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "naughty-words": "^1.2.0"
  }
}
